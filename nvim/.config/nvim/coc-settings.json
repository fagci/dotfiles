{
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.autoTrigger": "always",
  "suggest.detailField": "abbr",
  "suggest.echodocSupport": true,
  "suggest.enablePreselect": true,
  "suggest.enablePreview": false,
  "suggest.fixInsertedWord": true,
  "suggest.floatEnable": false,
  "suggest.minTriggerInputLength": 2,
  "suggest.noselect": false,
  "suggest.snippetIndicator": " >",
  "diagnostic.hintSign": "?",
  "diagnostic.errorSign": "x",
  "diagnostic.warningSign": "!",
  "diagnostic.infoSign": "i",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": " >> ",
  "diagnostic.messageDelay": 1000,
  "eslint.autoFix": false,
  "eslint.autoFixOnSave": false,
  "eslint.enable": true,
  "emmet.includeLanguages": {
    "html": "html",
    "php": "html",
    "gohtmltmpl": "html",
    "markdown": "html",
    "vue-html": "html",
    "css": "css"
  },
  "coc.preferences.rootPatterns": [
    "composer.json",
    "package.json",
    ".git"
  ],
  "snippets.shortcut": "ﲖ ",
  "suggest.completionItemKindLabels": {
    "class": " ",
    "color": " ",
    "constant": "c ",
    "default": " ",
    "enum": "E ",
    "enumMember": " ",
    "event": "鬒",
    "field": "= ",
    "file": " ",
    "folder": " ",
    "function": "",
    "interface": " ",
    "keyword": "",
    "method": "",
    "module": " ",
    "operator": " ",
    "property": " ",
    "reference": "渚",
    "snippet": " ",
    "struct": "פּ ",
    "text": "t ",
    "typeParameter": " ",
    "unit": " ",
    "value": " ",
    "variable": " "
  },
  "intelephense.telemetry.enabled": false,
  "intelephense.files.exclude": [
    "**/.git/**",
    "**/.svn/**",
    "**/.hg/**",
    "**/CVS/**",
    "**/.DS_Store/**",
    "**/node_modules/**",
    "**/bower_components/**",
    "**/vendor/**/{Test,test,Tests,tests}/**",
    "**/storage/**",
    "**/database/migrations/**",
    "**/.history/**",
    "**/vendor/**/vendor/**"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "php",
    "phtml",
    "html",
    "typescript",
    "javascript",
    "python",
    "json",
    "yaml",
    "vue",
    "gohtmltmpl"
  ],
  "diagnostic-languageserver.filetypes": {
    "css": "stylelint",
    "yaml": "ansible-lint",
    "asterisk": "asterisklint",
    "vim": "vint",
    "sh": "shellcheck",
    "python": "pylint",
    "php": [
      "tidy"
    ],
    "html": [
      "tidy"
    ],
    "gohtmltmpl": [
      "tidy"
    ],
    "javascript": [
      "eslint"
    ],
    "typescript": [
      "tslint"
    ]
  },
  "python.pythonPath": "python3",
  "python.jediPath": "/usr/lib/python3.8/site-packages/jedi",
  "python.linting.pylintArgs": [
    "--load-plugins",
    "pylint-flask"
  ],
  "python.autoComplete.addBrackets": true,
  "git.semanticCommit.filetypes": [
    "gitcommit",
    "markdown"
  ],
  "git.enableGlobalStatus": false
}
